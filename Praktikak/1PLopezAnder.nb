Cell[TextData[{
 "Zenbaki konplexuak ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptZ]", "\[LongEqual]", 
    RowBox[{"\[ScriptA]", "+", "\[ScriptB]\[ScriptI]"}]}], TraditionalForm]]],
 " motako zenbakiak dira, non ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[ScriptI]", "2"], "\[LongEqual]", 
    RowBox[{"-", "1"}]}], TraditionalForm]]],
 " baita; ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptZ]", "\[LongEqual]", 
    RowBox[{"\[ScriptA]", "+", "\[ScriptB]\[ScriptI]"}]}], TraditionalForm]]],
 " zenbaki konplexua ",
 StyleBox["OXY",
  FontSlant->"Italic"],
 " planoko ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"\[ScriptA]", ",", "\[ScriptB]"}], ")"}], TraditionalForm]]],
 " puntuarekin bat dator. \[ScriptZ]-ren \[ScriptA] koordenatua zati erreala \
da, eta \[ScriptZ]-ren \[ScriptB] koordenatua zati irudikaria. ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptZ]", "\[LongEqual]", 
    RowBox[{"\[ScriptA]", "+", "\[ScriptB]\[ScriptI]"}]}], TraditionalForm]]],
 " adierazpenari binomial deritzo, eta (\[ScriptA],\[ScriptB]) adierazpenari \
geometriko. ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 "-k balio horiek ematen dituzten funtzioak definiturik dauzka: ",
 StyleBox["Re",
  FontWeight->"Bold"],
 " eta ",
 StyleBox["Im",
  FontWeight->"Bold"],
 ", zati erreala eta zati irudikaria. Kontuan izan guk \
\[OpenCurlyDoubleQuote]\[ScriptI]\[CloseCurlyDoubleQuote] idazten duguna ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 "-k \[OpenCurlyDoubleQuote]",
 StyleBox["I",
  FontWeight->"Bold"],
 "\[CloseCurlyDoubleQuote] idazten duela."
}], "Text",
 CellChangeTimes->{{3.5825274794084997`*^9, 3.5825275615178747`*^9}, {
