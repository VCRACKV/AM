Cell["Zenbaki konplexuen logaritmo nepertarra", "Section",
 CellChangeTimes->{{3.582873931953125*^9, 3.582873940875*^9}}],

Cell[TextData[{
 "Atal honetan zenbaki konplexuen logaritmo nepertarra kalkulatuko dugu. \
Ondoren logaritmoaren emaitzaren irudia egingo dugu. Horrez gain, \
logaritmoaren balio nagusia ere kalkulatuko dugu.\nLogaritmo nepertarra \
kalkulatzeko adierazpide polarra erabiliko dugu, hau da, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptZ]", "\[LongEqual]", 
    SubscriptBox["\[Rho]", "\[Theta]"]}], TraditionalForm]]],
 " idazkera erabiliko dugu. Logaritmo nepertarraren formula hau da:\n\t\t\t\t\
\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"ln", " ", "\[ScriptZ]"}], " ", "\[LongEqual]", 
    RowBox[{"ln", " ", 
     SubscriptBox["\[Rho]", "\[Theta]"]}], "\[LongEqual]", 
    RowBox[{
     RowBox[{"ln", " ", "\[Rho]"}], " ", "+", " ", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[Theta]", "+", 
        RowBox[{"2", "\[ScriptK]\[Pi]"}]}], ")"}], "\[ScriptI]"}]}]}], 
   TraditionalForm]]],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptK]", "\[Element]", "\[DoubleStruckCapitalZ]"}], 
   TraditionalForm]]],
 "  izanik. "
}], "Text",
 CellChangeTimes->{{3.582873962234375*^9, 3.582874052203125*^9}, {
   3.582874112609375*^9, 3.582874136296875*^9}, {3.5828742043125*^9, 
