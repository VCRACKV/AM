Cell["Ekuazioaren soluzioak", "Section",
 CellChangeTimes->{{3.5826130442366247`*^9, 3.5826130614084997`*^9}, {
  3.5850323263673573`*^9, 3.5850323304611073`*^9}}],

Cell[TextData[{
 "Ekuazioa ebazteko koordenatu polarrak erabiliko ditugu; beraz, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptZ]", "\[LongEqual]", 
    SubscriptBox["\[Rho]", "\[Theta]"]}], TraditionalForm]]],
 " eta ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["\[ScriptZ]", "_"], "\[LongEqual]", 
    SubscriptBox["\[Rho]", 
     RowBox[{"-", "\[Theta]"}]]}], TraditionalForm]]],
 " idatziko ditugu. Berreketaren definizioa erabiliz, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      SubscriptBox["\[Rho]", "\[Theta]"], ")"}], "\[ScriptN]"], 
    "\[LongEqual]", 
    SubscriptBox[
     SuperscriptBox["\[Rho]", "\[ScriptN]"], "\[ScriptN]\[Theta]"]}], 
   TraditionalForm]]],
 " da; beraz, bi ekuazio aterako ditugu: moduluen ekuazioa, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Rho]", "\[ScriptN]"], "\[LongEqual]", "\[Rho]"}], 
   TraditionalForm]]],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Rho]", "\[GreaterEqual]", "0"}], TraditionalForm]]],
 " izanik, eta argumentuen ekuazioa, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptN]\[Theta]", "\[LongEqual]", 
    RowBox[{
     RowBox[{"-", "\[Theta]"}], "+", 
     RowBox[{"2", "\[ScriptK]\[Pi]"}]}]}], TraditionalForm]]],
 ", ",
 Cell[BoxData[
