Cell["Bernstein-en oinarria", "Subsection",
 CellChangeTimes->{{3.572642938400947*^9, 3.572642940287896*^9}, {
   3.582363962947067*^9, 3.582363969667082*^9}, {3.582364526686262*^9, 
   3.58236453412647*^9}, {3.582364586255748*^9, 3.582364641761075*^9}, {
   3.58236505554532*^9, 3.582365072217963*^9}, 3.582368198682916*^9, 
   3.5824434234084997`*^9, 3.5824444027991247`*^9}],

Cell[TextData[{
 "Orokorrean, [",
 StyleBox["a,b",
  FontSlant->"Italic"],
 "] tartean definitzen diren polinomioak dira; baina, guk [",
 StyleBox["0,1",
  FontSlant->"Italic"],
 "] tartean definitutakoak erabiliko ditugu. Polinomio hauek ",
 StyleBox["n",
  FontSlant->"Italic"],
 " mailako polinomioak dira; beraz, oinarrian ",
 StyleBox["n+1",
  FontSlant->"Italic"],
 " polinomio behar dira. Polinomio hauen definizioa [",
 StyleBox["0,1",
  FontSlant->"Italic"],
 "] tarterako honakoa da:",
 "\n\t\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["B", "i"], "(", "x", ")"}], "=", 
     RowBox[{
      RowBox[{"(", GridBox[{
         {"n"},
         {"i"}
        }], ")"}], 
      SuperscriptBox[
       RowBox[{
        SuperscriptBox["x", "i"], "(", 
        RowBox[{"1", "-", "x"}], ")"}], 
       RowBox[{"n", "-", "i"}]]}]}], ","}], TraditionalForm]]],
 "\nnon ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", GridBox[{
      {"n"},
      {"i"}
     }], ")"}], TraditionalForm]]],
 " zenbaki konbinatorioa baita.\nTartea ",
 StyleBox["(0,1)",
  FontSlant->"Italic"],
 " bada eta 3 mailako polinomioak nahi baditugu, hauek dira oinarriko lau \
polinomioak:\n\t\t\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["B", "0"], "(", "x", ")"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"(", GridBox[{
          {"3"},
          {"0"}
         }], ")"}], 
       SuperscriptBox[
        RowBox[{
         SuperscriptBox["x", "0"], "(", 
         RowBox[{"1", "-", "x"}], ")"}], "3"]}], "=", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", "-", "x"}], ")"}], "3"]}]}], ";"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["B", "1"], "(", "x", ")"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"(", GridBox[{
          {"3"},
          {"1"}
         }], ")"}], 
       SuperscriptBox[
        RowBox[{
         SuperscriptBox["x", "1"], "(", 
         RowBox[{"1", "-", "x"}], ")"}], "2"]}], "=", 
      RowBox[{"3", 
       SuperscriptBox[
        RowBox[{"x", "(", 
         RowBox[{"1", "-", "x"}], ")"}], "2"]}]}]}], ";"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["B", "2"], "(", "x", ")"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"(", GridBox[{
          {"3"},
          {"2"}
         }], ")"}], 
       SuperscriptBox[
        RowBox[{
         SuperscriptBox["x", "2"], "(", 
         RowBox[{"1", "-", "x"}], ")"}], "1"]}], "=", 
      RowBox[{"3", 
       RowBox[{
        SuperscriptBox["x", "2"], "(", 
        RowBox[{"1", "-", "x"}], ")"}]}]}]}], ";"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["B", "3"], "(", "x", ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", GridBox[{
         {"3"},
         {"3"}
        }], ")"}], 
      SuperscriptBox[
       RowBox[{
        SuperscriptBox["x", "3"], "(", 
        RowBox[{"1", "-", "x"}], ")"}], "0"]}], "=", 
     RowBox[{
      SuperscriptBox["x", "3"], "."}]}]}], TraditionalForm]}]]
