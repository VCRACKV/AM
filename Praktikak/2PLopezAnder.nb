Cell[TextData[{
 "Maila bereko polinomioen multzoa bektore-espazio bat da. Bektore-espazioek \
oinarriak dituzte, horregatik polinomio bat oinarriko beste polinomio batzuen \
konbinazio gisa adieraz daiteke. Esate baterako, bektore-espazio horren \
oinarri kanonikoa honako polinomio sinple hauek osatzen dute:\n\t\t\t\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["P", "0"], "=", 
     SuperscriptBox["x", "0"]}], ";"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["P", "1"], "=", 
     SuperscriptBox["x", "1"]}], ";"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["P", "2"], "=", 
     SuperscriptBox["x", "2"]}], ";"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"\[Ellipsis]", " "}], TraditionalForm]}]],
 "\nPolinomio horietan oinarrituz, beste edozein polinomio horien konbinazio \
lineal gisa adieraz daiteke:\n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"P", "(", "x", ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["a", "k"], 
      SubscriptBox["P", "k"]}], "+", 
     RowBox[{
      SubscriptBox["a", 
       RowBox[{"k", "-", "1"}]], 
      SubscriptBox["P", 
       RowBox[{"k", "-", "1"}]]}], "+", 
     RowBox[{
      SubscriptBox["a", 
       RowBox[{"k", "-", "2"}]], 
      SubscriptBox["P", 
       RowBox[{"k", "-", "2"}]]}], "+", "\[CenterEllipsis]", "+", 
     RowBox[{
      SubscriptBox["a", "1"], 
      SubscriptBox["P", "1"]}], "+", 
     RowBox[{
      SubscriptBox["a", "0"], 
      RowBox[{
       SubscriptBox["P", "0"], ".", " "}]}]}]}], TraditionalForm]]]
}], "Text",
