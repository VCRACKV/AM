         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"bpy", "[", "t_", "]"}], "=", 
        RowBox[{"bp", "[", 
         RowBox[{"t", ",", "p0y", ",", "p1y", ",", "p2y", ",", "p3y"}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"maxx", "=", 
