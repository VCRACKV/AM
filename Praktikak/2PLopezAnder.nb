Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"marraztukurba", "[", "puntulista_List", "]"}], ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "p0x", ",", "p0y", ",", "p1x", ",", "p1y", ",", "p2x", ",", "p2y", ",", 
       "p3x", ",", "p3y", ",", "bpx", ",", "bpy", ",", "maxx", ",", "minx", 
       ",", "maxy", ",", "miny", ",", "kurba", ",", "inguratzailea"}], "}"}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"p0x", "=", 
        RowBox[{
         RowBox[{"puntulista", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"p0y", "=", 
        RowBox[{
         RowBox[{"puntulista", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "[", 
         RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"p1x", "=", 
        RowBox[{
         RowBox[{"puntulista", "[", 
          RowBox[{"[", "2", "]"}], "]"}], "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"p1y", "=", 
        RowBox[{
         RowBox[{"puntulista", "[", 
          RowBox[{"[", "2", "]"}], "]"}], "[", 
         RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"p2x", "=", 
        RowBox[{
         RowBox[{"puntulista", "[", 
          RowBox[{"[", "3", "]"}], "]"}], "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"p2y", "=", 
        RowBox[{
         RowBox[{"puntulista", "[", 
          RowBox[{"[", "3", "]"}], "]"}], "[", 
         RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"p3x", "=", 
        RowBox[{
         RowBox[{"puntulista", "[", 
          RowBox[{"[", "4", "]"}], "]"}], "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"p3y", "=", 
        RowBox[{
